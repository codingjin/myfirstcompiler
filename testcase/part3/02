def sumUpTo(int n) : int
	int sum;
	int i;
	sum = 0;
	for (i=1;i<=n;i=i+1) do
		sum = sum + i;
	end;
	return sum;
end

def A(int n) : int
	int s;
	int i;
	s = n;
	i = s+1;
	return s+i;
end

def main
	int n;
	int sum;
	int m;
	n = 20;
	m = 3;
	sum = sumUpTo(n);
	print(sum);
	m = A(m);
	print(m);
end
